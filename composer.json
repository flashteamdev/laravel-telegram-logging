{
    "name": "flashteamdev/laravel-telegram-logging",
    "description": "Send logs to Telegram chat via Telegram bot",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Logger\\": "src"
        }
    },
    "require": {
        "php": "^8.2",
        "monolog/monolog": "^3.0",
        "laravel/framework": "^10.0 | ^11.0 | ^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Logger\\TelegramLoggerServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.20"
    }
}
